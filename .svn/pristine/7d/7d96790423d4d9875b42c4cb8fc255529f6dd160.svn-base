package com.dp.persistence.vo;

import java.util.ArrayList;
import java.util.List;

import com.dp.persistence.domain.EquipmentStatus;

public class EquipmentVO {
	
	private String name;
	
	private List<EquipmentStatus> list = new ArrayList<EquipmentStatus>();

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<EquipmentStatus> getList() {
		return list;
	}

	public void setList(List<EquipmentStatus> list) {
		this.list = list;
	}
	
}
